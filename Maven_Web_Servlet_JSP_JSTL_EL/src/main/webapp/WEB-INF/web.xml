<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>serv</servlet-name>
  	<servlet-class>com.danielpm1982.Maven_Web_Servlet_JSP_JSTL_EL.controller.Serv</servlet-class>
